{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "types": [
      "hammerjs",
      "node"
    ],
    "baseUrl": "src",
    "paths": {
      "@app/service/app": [
        "app/shared/services/app-bundle.service"
      ],
      "@app/service/common": [
        "app/shared/services/common-bundle.service"
      ],
      "@app/service/ui": [
        "app/shared/services/ui-bundle.service"
      ],
      "@app/config": [
        "app/shared/config/app.config"
      ],
      "@app/routing": [
        "app/shared/config/app-routing.config"
      ],
      "@app/enums": [
        "app/shared/enums/app.enum"
      ],
      "@app/models": [
        "app/shared/models/app.model"
      ],
      "@app/models/ui": [
        "app/shared/models/app-ui.model"
      ],
      "@app/interfaces": [
        "app/shared/interfaces/app.interface"
      ],
      "@app/ui": [
        "app/shared/ui/shared-ui"
      ],
      "@app/pagination": [
        "app/shared/components/pagination/bootstrap/shared-app-pagination.module"
      ],
      "@app/fileUpload": [
        "app/shared/components/file-upload/shared-app-file-upload.module"
      ],
      "@app/loading/CubeGrid": [
        "app/shared/components/loading/cube-grid/shared-app-loading-cube-grid.module"
      ],
      "@app/baseComponent": [
        "app/app-base.component"
      ],
      "@app/webBaseComponent": [
        "app/components/web/app-web-base.component"
      ],
      "@app/cmsBaseComponent": [
        "app/components/cms/app-cms-base.component"
      ],
      "@app/passportBaseComponent": [
        "app/components/passport/app-passport-base.component"
      ],
    }
  }
}
